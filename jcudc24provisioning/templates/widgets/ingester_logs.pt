<span>
    <p>Click on the dataset you want to view the logs for below.</p>

    <div tal:repeat="dataset field.schema.datasets" class="logs_dataset content_container">
        <h3>${dataset.title}</h3>
        <p class="error" tal:condition="exists:dataset.logs_error">${dataset.logs_error}</p>
        <div tal:condition="exists:dataset.logs">
            <p tal:repeat="log dataset.logs" class="level-${log['level']}">${log['timestamp']} ${log['message']}</p>
        </div>
    </div>

    <script type="text/javascript">
            deform.addCallback(
                    '${field.oid}',
                    function (oid) {
                        $(".logs_dataset p").hide();
                        $('.logs_dataset').addClass('collapsed');
                        $(".logs_dataset").attr("onclick", "$('.logs_dataset').not($(this)).find('p').hide(); $('.logs_dataset').not($(this)).addClass('collapsed');  $(this).find('p').toggle(); $('.logs_dataset').toggleClass('collapsed');")
                    });
    </script>
</span>
