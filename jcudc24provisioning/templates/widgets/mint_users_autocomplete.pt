<span tal:omit-tag="">
    <input type="text"
           name="${field.name}"
           value="${cstruct}"
           tal:attributes="size field.widget.size;
                           class field.widget.css_class"

           id="${field.oid}"/>

    <script tal:condition="field.schema.default" type="text/javascript">
        submitClearsDefaultValues("${field.oid}", "${field.schema.default}");
    </script>
    <script tal:condition="field.widget.values" type="text/javascript">
        deform.addCallback(
                '${field.oid}',
                function (oid) {
                    $('#' + oid).autocomplete({source: getMintUsers});
                    $('#' + oid).autocomplete("option", ${options});
                }
        );

        function getMintUsers() {
            alert('here');
//            http://tdh-ms-dev.hpc.jcu.edu.au:9001/solr/fascinator/select?q=dr%20stephen*
        }
    </script>

</span>
